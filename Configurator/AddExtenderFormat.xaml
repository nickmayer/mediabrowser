<Window x:Class="Configurator.AddExtenderFormat"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="AddExtenderFormat" Height="207" Width="366">
    <Grid>
    	<Label Margin="12,22,0,0" VerticalAlignment="Top" Height="37" Content="Add format to be handled natively by the extender" FontSize="14" HorizontalAlignment="Left" Width="335" />
        <TextBox Margin="12,65,0,0" Name="formatName" FontSize="14" Height="33" VerticalAlignment="Top" HorizontalAlignment="Left" Width="318" />
        <Button Height="32" HorizontalAlignment="Left" Margin="12,0,0,27" Name="OK" VerticalAlignment="Bottom" Width="90" Click="ok_Click">OK</Button>
        <Button Height="32" HorizontalAlignment="Left" Margin="240,0,0,27" Name="Cancel" VerticalAlignment="Bottom" Width="90" Click="cancel_Click">Cancel</Button>
    </Grid>
</Window>
