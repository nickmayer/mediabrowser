<Window x:Class="Configurator.AddExtenderFormat"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Add Extender Format" Height="175" Width="450" ResizeMode="NoResize">
    <Grid>
    	<Label Margin="20,20,20,0" VerticalAlignment="Top" Height="28" Content="Add format to be handled natively by the extender" FontSize="14" />
        <TextBox Margin="20,50,20,0" Name="formatName" FontSize="14" Height="28" VerticalAlignment="Top" />
        <Button Height="32" HorizontalAlignment="Right" Margin="0,0,20,20" Name="OK" VerticalAlignment="Bottom" Width="80" Click="ok_Click">OK</Button>
        <Button Height="32" HorizontalAlignment="Right" Margin="0,0,110,20" Name="Cancel" VerticalAlignment="Bottom" Width="80" Click="cancel_Click">Cancel</Button>
    </Grid>
</Window>
