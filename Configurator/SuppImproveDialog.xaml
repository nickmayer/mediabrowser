        <TextBlock Margin="35,62,33,57" Name="tbMessage" FontSize="14" Text="This is a message..." TextWrapping="Wrap" />
        <Button Height="35" HorizontalAlignment="Right" Margin="0,0,25,12" Name="btnOK" VerticalAlignment="Bottom" Width="85" FontSize="14" IsDefault="True" Click="btnOK_Click" Content="Yes I'll Help!"></Button>
